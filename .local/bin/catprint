#!/bin/bash

randomnum=$((1 + RANDOM % 3))

if [[ $randomnum == 1 ]]; then
    printf "      |\      _,,,---,,_\n"
    printf "ZZZzz /,\`.-'\`'    -.  ;-;;,_\n"
    printf "     |,4-  ) )-,_. ,\ (  \`'-'\n"
    printf "    '---''(_/--'  \`-'\_)  Nala \n"

elif [[ $randomnum == 2 ]]; then
    printf "    |\__/,|   (\`\\ \n"
    printf "  _.|o o  |_   ) )\n"
    printf " -(((---(((--------\n"


else
    printf " |\__/,|   (\`\\ \n"
    printf " |_ _  |.--.) ) \n"
    printf " ( T   )     / \n"
    printf "(((^_(((/(((_/ \n"
fi